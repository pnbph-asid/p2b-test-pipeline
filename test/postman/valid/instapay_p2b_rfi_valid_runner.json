		{
			"name": "200 - Valid SN - Validation Request",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"var requestBody = JSON.parse(pm.request.body);",
							"var instructionId = requestBody.instructionId;",
							"",
							"",
							"pm.test(\" Valid Biller Subscriber Number Status Code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							" var jsonData = pm.response.json();",
							"",
							"",
							"pm.test(\" Valid Biller Subscriber Number Status is IN-PROGRESS\", function () {",
							"    pm.expect(jsonData.txnStatus).to.have.eql(\"IN-PROGRESS\");});",
							"",
							"pm.test(\" Valid Biller Subscriber Number Instruction ID is \" + instructionId, function () {",
							"    pm.expect(jsonData.instructionId).to.have.eql(instructionId);});",
							"",
							"",
							"    pm.test(\" Valid Biller Subscriber Number Trace Number has a value\", function () {",
							"    pm.expect(jsonData.traceNo).to.not.eql(null);});",
							"",
							"",
							"    pm.test(\" Valid Biller Subscriber Number Reason Code Empty\", function () {",
							"    pm.expect(jsonData.reason).to.have.eql(\"\");});",
							"",
							"pm.collectionVariables.set(\"validateInstructionId\", jsonData.instructionId);",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"const currentDate = new Date();\r",
							"\r",
							"const month = String(currentDate.getMonth() + 1).padStart(2, '0'); \r",
							"const day = String(currentDate.getDate()).padStart(2, '0');\r",
							"\r",
							"const year = String(currentDate.getFullYear()).slice(-2);\r",
							"\r",
							"const txnDate = `${month}${day}`;\r",
							"\r",
							"const hours = String(currentDate.getHours()).padStart(2, '0');\r",
							"const minutes = String(currentDate.getMinutes()).padStart(2, '0');\r",
							"const seconds = String(currentDate.getSeconds()).padStart(2, '0');\r",
							"\r",
							"const txnTime = `${hours}${minutes}${seconds}`; \r",
							"//TODO autogeneration of txnDateTime\r",
							"pm.environment.set('txnDate', txnDate);\r",
							"pm.environment.set('txnTime', txnTime);\r",
							"\r",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
